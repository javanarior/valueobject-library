<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/settings/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <profiles>
        <profile>
            <id>bintray-javanarior</id>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>bintray-javanarior</id>
                    <name>bintray</name>
                    <url>http://dl.bintray.com/javanarior/maven</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>bintray-javanarior</id>
                    <name>bintray-plugins</name>
                    <url>http://dl.bintray.com/javanarior/maven</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>



    <activeProfiles>
        <activeProfile>bintray-javanarior</activeProfile>
    </activeProfiles>

</settings>
